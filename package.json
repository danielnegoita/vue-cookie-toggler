{
  "name": "vue-cookie-toggler",
  "version": "0.2.2",
  "description": "GDPR cookies consent modal.",
  "author": "Daniel Negoita <hello@danielnegoita.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielnegoita/vue-cookie-toggler.git"
  },
  "bugs": {
    "url": "https://github.com/danielnegoita/vue-cookie-toggler.git/issues"
  },
  "keywords": [
    "vue",
    "gdpr",
    "modal",
    "categorize",
    "block",
    "consent",
    "cookie"
  ],
  "main": "dist/vue-cookie-toggler.ssr.js",
  "browser": "dist/vue-cookie-toggler.esm.js",
  "module": "dist/vue-cookie-toggler.esm.js",
  "unpkg": "dist/vue-cookie-toggler.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "postscribe": "^2.0.8",
    "vue-js-modal": "^1.3.34",
    "vue-switches": "^2.0.1",
    "vue2-collapse": "^1.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/eslint-config-prettier": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "minimist": "^1.2.5",
    "prettier": "^2.2.1",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass-loader": "10.1.1",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  }
}
